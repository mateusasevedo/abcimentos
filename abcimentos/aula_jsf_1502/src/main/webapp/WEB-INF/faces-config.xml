<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">
	<navigation-rule>
		<from-view-id>/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>sucesso</from-outcome>
			<to-view-id>/system/principal.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-outcome>falha</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>principal</from-outcome>
			<to-view-id>/system/principal.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>logout</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>cadlivro</from-outcome>
			<to-view-id>/system/livro/cadastro.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/system/livro/cadastro.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>sucesso</from-outcome>
			<to-view-id>/system/principal.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/system/livro/cadastro.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>falha</from-outcome>
			<to-view-id>/system/livro/cadastro.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>*</display-name>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>listproduto</from-outcome>
			<to-view-id>/system/produto/principal.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>system/produto/principal.xhtml</display-name>
		<from-view-id>/system/produto/principal.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>cadproduto</from-outcome>
			<to-view-id>/system/produto/cadastro.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>system/produto/cadastro.xhtml</display-name>
		<from-view-id>/system/produto/cadastro.xhtml</from-view-id>
		<navigation-case>
			<to-view-id>/system/produto/principal.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>system/produto/principal.xhtml</display-name>
		<from-view-id>/system/produto/principal.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>editar</from-outcome>
			<to-view-id>/system/produto/edicao.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>*</display-name>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>listcliente</from-outcome>
			<to-view-id>/system/cliente/principal.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>system/cliente/principal.xhtml</display-name>
		<from-view-id>/system/cliente/principal.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>cadcliente</from-outcome>
			<to-view-id>/system/cliente/cadastro.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>system/cliente/cadastro.xhtml</display-name>
		<from-view-id>/system/cliente/cadastro.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>sucesso</from-outcome>
			<to-view-id>/system/cliente/principal.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>system/cliente/principal.xhtml</display-name>
		<from-view-id>/system/cliente/principal.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>editar</from-outcome>
			<to-view-id>/system/cliente/edicao.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>system/produto/edicao.xhtml</display-name>
		<from-view-id>/system/produto/edicao.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>sucesso</from-outcome>
			<to-view-id>/system/produto/principal.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>system/venda/principal.xhtml</display-name>
		<from-view-id>/system/venda/principal.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>cadvenda</from-outcome>
			<to-view-id>/system/venda/cadastro.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>system/venda/principal.xhtml</display-name>
		<from-view-id>/system/venda/principal.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>editar</from-outcome>
			<to-view-id>/system/venda/edicao.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>*</display-name>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>listvenda</from-outcome>
			<to-view-id>/system/venda/principal.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>system/venda/edicao.xhtml</display-name>
		<from-view-id>/system/venda/edicao.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>sucesso</from-outcome>
			<to-view-id>/system/venda/principal.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>system/venda/cadastro.xhtml</display-name>
		<from-view-id>/system/venda/cadastro.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>sucesso</from-outcome>
			<to-view-id>/system/venda/principal.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>system/cliente/edicao.xhtml</display-name>
		<from-view-id>/system/cliente/edicao.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>sucesso</from-outcome>
			<to-view-id>/system/cliente/principal.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<validator>
		<validator-id>cpf</validator-id>
		<validator-class>validator.CPFValidator</validator-class>
	</validator>
	
</faces-config>
